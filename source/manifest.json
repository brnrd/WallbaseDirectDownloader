{
	"name": "Wallbase Download from Search",
	"version": "0.0.2",
	"manifest_version": 2,
	"description": "Allows you to download the wallpapers on Wallbase directly from the search pages",
	"content_scripts": [{
		"matches": ["http://wallbase.cc/search*","http://wallbase.cc/random*", "http://wallbase.cc/tags*", "http://wallbase.cc/toplist*"],
		"js": ["jquery.js","content.js"],
		"css": ["wbs.css"]
		}],
	"web_accessible_resources": ["jquery-2.0.3.min.map", "bg-hover-top.png"]
}